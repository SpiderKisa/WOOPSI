<%- layout('/layouts/boilerplate') %>
    <div class="row">
        <div class="col-3"></div>
        <div class="col-6">
            <div class="row">
                <div class="col-4">
                    <img src="/images/error.svg" alt="error" class="img-fluid">
                </div>
                <div class="col-8">
                    <div class="row">
                        <div class="col">Ошибка <%= err.statusCode %>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <%= err.message %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-3"></div>
    </div>